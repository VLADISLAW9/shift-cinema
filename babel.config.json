{
	"presets": [
			"@babel/preset-typescript",
			[
				"@babel/preset-env", 
				{ 
					"modules": false,
					"targets": { "node": "current" }
				}
			],
			[
				"@babel/preset-react",
				{
					"runtime": "automatic"
				}
			]
	],
	"plugins": [
    "@babel/plugin-transform-modules-commonjs",
		"transform-vite-meta-env"
  ]
}